<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="root" prefHeight="400.0" prefWidth="600.0"  xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cliente.ui.pantallas.principal.PrincipalController">
    <center>
        <StackPane prefHeight="150.0" prefWidth="200.0">
            <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="background" BorderPane.alignment="CENTER" />
        </StackPane>
    </center>
    <top>
        <MenuBar fx:id="menuPrinc" prefHeight="25.0" prefWidth="261.0" BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="Listas">
                    <items>
                        <MenuItem fx:id="listar" mnemonicParsing="false" onAction="#menuClick" text="listar" />
                        <MenuItem fx:id="add" mnemonicParsing="false" onAction="#menuClick" text="add" />
                           </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
</BorderPane>
